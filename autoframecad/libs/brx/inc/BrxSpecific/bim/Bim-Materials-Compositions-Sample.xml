<thirdPartySampleLibrary>
    <meta>
        <unit>Millimeter</unit>
    </meta>
    <material id="1">
        <name>Maple</name>
        <type>Wood</type>
        <bim_asset>
            <default_function>Structure</default_function>
            <variable_thickness>true</variable_thickness>
            <thickness>40</thickness>
        </bim_asset>
    </material>
    <composition>
        <name>Maple with Oak finish</name>
        <ply>
            <material>
                <name>Oak finish</name>
                <type>Wood</type>
                <bim_asset>
                    <default_function>Finish1</default_function>
                    <variable_thickness>false</variable_thickness>
                    <thickness>4</thickness>
                </bim_asset>
            </material>
            <function>Finish1</function>
            <thickness>4</thickness>
        </ply>
        <ply>
            <material id="1"/>
            <function>Structure</function>
            <thickness>20</thickness>
        </ply>
        <variable_ply>1</variable_ply>
    </composition>
    <composition>
        <name>Plain Maple</name>
        <ply>
            <material name="Maple"/>
            <thickness>20</thickness>
        </ply>
        <variable_ply>0</variable_ply>
    </composition>
</thirdPartySampleLibrary>
